package fr.epita.quiz.datamodel;

/**
 * studentAnswer();
 * @author emmanuel
 *Sets the answer of an mcqquestion
 */

public class studentAnswer {

	String answer;
	
	public studentAnswer(String answer) {
		super();
		this.answer = answer;
	
	}

	public void setAnswer(String answer) {
		this.answer = answer;
	}

	public String getAnswer() {
		return answer;
	}
	
}
